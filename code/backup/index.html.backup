<div id="homeScreen">
	<div id="searchContainer">
		<div id="searchTxtWrapper">
			<input type="text" name="search" value id="search" onkeyup="ontxtchange();" placeholder="Search...">
		</div>
	</div>

	<div id="linkrow">
		<a href="javascript:placeholder()" style="float: left;">MyBuzz</a>
		<a href="javascript:placeholder()" style="float: right;">Advanced Search</a>
	</div>

	<div id="homeList">
		<div class="homeItem" onclick="show_thebuzz()">
			<div class="homeName">The Buzz</div>
			<div class="placeDistance">&nbsp;</div>
			<div class="homeLinkButton">&nbsp;</div>
		</div>

		<div class="homeItem" onclick="placeholder()">
			<div class="homeName">Events</div>
			<div class="placeDistance">&nbsp;</div>
			<div class="homeLinkButton">&nbsp;</div>
	</div>

		<div class="homeItem" onclick="placeholder()">
			<div class="homeName">Buzz-Mart</div>
			<div class="placeDistance">&nbsp;</div>
			<div class="homeLinkButton">&nbsp;</div>
		</div>

		<div class="homeItem" onclick="placeholder()">
			<div class="homeName">Sports</div>
			<div class="placeDistance">&nbsp;</div>
			<div class="homeLinkButton">&nbsp;</div>
		</div>

		<div class="homeItem" id="Buzzniz" onclick="placeholder()">
			<div class="homeName">Buzzniz</div>
			<div class="placeDistance">&nbsp;</div>
			<div class="homeLinkButton">&nbsp;</div>		
		</div>
	</div>

</div>


<div id="content">
</div>






